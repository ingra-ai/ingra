{
  "env": {
    "browser": true,
    "es6": true,
    "es2020": true
  },
  "extends": [
    "eslint:recommended",
    "next"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error",
    "no-case-declarations": "warn"
  }
}
